---
import type { Icon } from '../../../types/component';

const { strokeWidth = 1.5, size = 20, class: className }: Icon = Astro.props;
---

<svg
  xmlns='http://www.w3.org/2000/svg'
  width={size}
  height={size}
  viewBox='0 0 24 24'
  fill='none'
  stroke='currentColor'
  stroke-width={strokeWidth}
  stroke-linecap='round'
  stroke-linejoin='round'
  class:list={['lucide lucide-arrow-right', className]}
>
  <path d='M5 12h14' />
  <path d='m12 5 7 7-7 7' />
</svg>